<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quizz"
        tools:targetApi="31">
<!--    android:theme="@style/Theme.Quizz"-->
        <activity
            android:name=".Play_Created_Quiz"
            android:exported="false" />
        <activity
            android:name=".quiz_is_ready"
            android:exported="false" />
        <activity
            android:name=".quiz_ques"
            android:exported="false" />
        <activity
            android:name=".Create_quiz"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".score_activity"
            android:exported="false"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".MainActivity2"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.QuizActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>